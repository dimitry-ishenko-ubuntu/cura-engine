#!/bin/sh

set -e

at_exit() {
    echo "info: test exiting"
}

trap at_exit INT TERM EXIT

echo "info: Generating gcode from stl file"
# CuraEngine prints everything to stderr - not really useful.
CuraEngine slice -j debian/tests/fdmprinter.def.json -l tests/testModel.stl -o $AUTOPKGTEST_TMP/foo.gcode 2>&1
